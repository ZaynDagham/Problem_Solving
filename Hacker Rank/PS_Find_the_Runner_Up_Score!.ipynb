      "source": [
        "n = int(input())\n",
        "arr = map(int, input().split())\n",
        "List = list(arr)\n",
        "\n",
        "List.sort()\n",
        "h_number = max(List)\n",
        "\n",
        "for i in range(len(List)-1, -1, -1):\n",
        "    if List[i] < h_number:\n",
        "        break\n",
        "\n",
        "print(List[i])"
      ]
